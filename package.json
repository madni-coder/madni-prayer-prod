{
    "name": "my-next-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npm run move-api && NEXT_DISABLE_TURBOPACK=1 next dev",
        "move-api": "if [ -d 'src/utils/api' ]; then rm -rf src/app/api && mv src/utils/api src/app/api; fi",
        "move-api-back": "if [ -d 'src/app/api' ]; then rm -rf src/utils/api && mv src/app/api src/utils/api; fi",
        "delete:build": "rm -rf .next && rm -rf out",
        "build": "npm run move-api && prisma generate && prisma migrate && next build",
        "build:tauri": "npm run delete:build && npm run move-api-back && prisma generate && next build",
        "start": "next start",
        "lint": "eslint",
        "db:generate": "prisma generate",
        "tauri": "tauri"
    },
    "dependencies": {
        "@prisma/client": "^6.16.2",
        "@supabase/supabase-js": "^2.58.0",
        "@tauri-apps/plugin-opener": "^2.5.0",
        "@tiptap/extension-underline": "^3.7.2",
        "@tiptap/pm": "^3.7.2",
        "@tiptap/react": "^3.7.2",
        "@tiptap/starter-kit": "^3.7.2",
        "@types/react-dom": "^19.2.2",
        "busboy": "^1.6.0",
        "emoji-picker-react": "^4.14.2",
        "formidable": "^3.5.4",
        "framer-motion": "^12.23.12",
        "html2canvas": "^1.4.1",
        "lucide-react": "^0.542.0",
        "next": "15.5.0",
        "pdfjs-dist": "^5.4.296",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-easy-crop": "^5.5.2",
        "react-icons": "^5.5.0",
        "react-image-crop": "^11.0.10"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@tauri-apps/cli": "^2.8.4",
        "@types/node": "24.3.0",
        "@types/react": "^19.2.2",
        "daisyui": "^5.0.51",
        "eslint": "^9",
        "eslint-config-next": "15.5.0",
        "prisma": "^6.16.2",
        "tailwindcss": "^4",
        "typescript": "5.9.3"
    }
}
