{
    "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
    "productName": "Raahe Hidayat",
    "version": "6.7.0",
    "identifier": "com.prayer.madni",
    "build": {
        "frontendDist": "../out",
        "devUrl": "https://raahehidayat.vercel.app",
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build:tauri"
    },
    "app": {
        "windows": [
            {
                "title": "Raahe Hidayat",
                "width": 800,
                "height": 600,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "resources": [
            "../public/sehri.mp3",
            "../public/irftari.mp3",
            "gen/apple/assets/sehri.caf",
            "gen/apple/assets/irftari.caf"
        ],
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "android": {
            "minSdkVersion": 24,
            "versionCode": 6007
        },
        "iOS": {
            "developmentTeam": "R8953SWC87",
            "minimumSystemVersion": "14.0"
        }
    }
}