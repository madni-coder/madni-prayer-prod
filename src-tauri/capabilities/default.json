{
    "identifier": "default",
    "description": "enables the default permissions",
    "windows": ["main"],
    "permissions": [
        "core:default",
        "opener:default",
        "os:default",
        "haptics:default",
        "notification:default",
        {
            "identifier": "opener:allow-open-url",
            "allow": [
                { "url": "comgooglemaps:*" },
                { "url": "geo:*" },
                { "url": "google.navigation:*" }
            ]
        }
    ]
}
